SELECT V.NAVN AS VARENAVN, V.EAN, B.BUTIKKID, B.BUTIKKSTED, B.BUTIKKADRESSE, L.LAGERID, KR.KUNDENAVN, BA.ANSATTNUMMER, BA.ANSATTNAVN

FROM BUTIKKANSATT AS BA JOIN(3
  KUNDEKLUBB AS KR JOIN (

    LAGER AS L JOIN (

     VARE AS V JOIN ( 

        VAREBEHOLDNING AS VB JOIN BUTIKK AS B 

          ON VB.BUTIKKID = B.BUTIKKID)

            ON V.VAREID = VB.VAREID) 

            ON B.LAGERID = L.LAGERID)

                ON KR.BUTIKKID = B.BUTIKKID)

                  ON BA.BUTIKKID = B.BUTIKKID


SELECT* FROM BUTIKK;

SELECT* FROM BUTIKKANSATT;

SELECT* FROM BUTIKK AS B JOIN BUTIKKANSATT AS BA ON  B.BUTIKKID = BA.BUTIKKID;

SELECT* FROM BUTIKK RIGHT JOIN BUTIKKANSATT ON  BUTIKK.BUTIKKID = BUTIKKANSATT.BUTIKKID;

SELECT* FROM BUTIKK LEFT JOIN BUTIKKANSATT ON  BUTIKK.BUTIKKID = BUTIKKANSATT.BUTIKKID;

SELECT* FROM BUTIKK JOIN BUTIKKANSATT ON BUTIKK.BUTIKKSTED = BUTIKKANSATT.ANSATTETTERNAVN;

SELECT BUTIKK.BUTIKKSTED, BUTIKK.BUTIKKID, BUTIKKANSATT.ANSATTNAVN

FROM BUTIKK

FULL OUTER JOIN BUTIKKANSATT

ON  BUTIKK.BUTIKKID = BUTIKKANSATT.BUTIKKID;

SELECT* FROM BUTIKK RIGHT JOIN BUTIKKANSATT ON  BUTIKK.BUTIKKID = BUTIKKANSATT.BUTIKKID

UNION ALL

SELECT* FROM BUTIKK LEFT JOIN BUTIKKANSATT ON  BUTIKK.BUTIKKID = BUTIKKANSATT.BUTIKKID;
