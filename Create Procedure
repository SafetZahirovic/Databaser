delimiter ::
create procedure Tilbud
(
	in t_Navn Varchar (20),
  in t_Prosent Decimal (4, 1)
)
BEGIN 
	update Vare1
	set Pris = Round (Pris - (1 + t_Prosent / 100), 2)
	where Navn = t_Navn;
END ::
delimiter ;


SELECT*
from Vare1;


CALL Tilbud ( "Deigfat", 30);


SELECT*
from Vare1;

drop procedure Tilbud;
